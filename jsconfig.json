{
  "compilerOptions": {
    "baseUrl": "./",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@service/*": ["src/service/*"],
      "@common/*": ["src/common/*"],
