{
  "name": "vite-project",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --mode dev --host --open",
    "build": "tsc && vite build --mode pro",
    "preview": "vite preview",
    "release": "npm install && npm run build",
    "lint": "npm run lint:style && npm run lint:js",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,json}'",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:style": "stylelint --fix \"src/**/*.less\" --custom-syntax postcss-less",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx --format=pretty && npm run lint:style",
    "lint-staged:js": "eslint . --ext .js,.jsx,.ts,.tsx "
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@ant-design/pro-components": "^1.1.5",
    "@ant-design/pro-table": "^3.3.1",
    "antd": "^4.24.3",
    "axios": "^1.2.0",
    "dompurify": "^2.4.4",
    "history": "^5.3.0",
    "js-base64": "^3.7.5",
    "js-cookie": "^3.0.1",
    "less": "^4.1.3",
    "querystring": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "sm-crypto": "^0.3.12",
    "typescript": "^4.6.4",
    "use-merge-value": "^1.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylelint": "^14.15.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.6.0",
    "vite": "^3.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imp": "^2.3.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "lint-staged": {
    "**/*.less": "npm run lint:style",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,json}": [
      "prettier --write"
    ]
  }
}
